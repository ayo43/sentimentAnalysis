        "x = sum(data[\"Positive\"])\n",
        "y = sum(data[\"Negative\"])\n",
        "\n",
        "def sentiment_score(a, b):\n",
        "    if (a>b):\n",
        "        print(\"Positive\")\n",
        "    elif (b>a):\n",
        "        print(\"Negative\")\n",
        "\n",
        "\n",
        "print(\"Positive: \", x)\n",
        "print(\"Negative: \", y)\n",
        "\n",
        "sentiment_score(x, y)"
